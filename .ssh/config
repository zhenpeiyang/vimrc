# These options come from https://github.pie.apple.com/pie/piessh/blob/master/docs/CLIENT_CONFIG.md
Host *.app.apple.com *container*.pie.apple.com
  ConnectTimeout 60
  ControlMaster no
  ControlPath none
  ForwardAgent no
  ForwardX11 no
  IdentityFile none
  KbdInteractiveAuthentication no
  PasswordAuthentication no
  PubkeyAuthentication yes
  ServerAliveInterval 30
  VerifyHostKeyDNS no
  LogLevel INFO
Host ssh.*.app.apple.com
  ProxyCommand ssh -W %r:1 proxy.%h
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
Host live-container.*.pie.apple.com
  ProxyCommand ssh -W %r:1 proxy-%h
  UserKnownHostsFile /dev/null
  StrictHostKeyChecking no
Host proxy.ssh.*.app.apple.com proxy-live-container.*.pie.apple.com
  UserKnownHostsFile /Users/yzp/.turibolt/ssh_known_hosts.v2
  StrictHostKeyChecking yes
Host *-bastion.isg.apple.com *-bastion.geo.apple.com
  UserKnownHostsFile /Users/yzp/.turibolt/ssh_known_hosts.v2
  StrictHostKeyChecking yes
  ConnectTimeout 60
Host *.bolt-pods.turi-bolt.svc.int.*.applecloud.io *.bolt-pods.default.svc.cluster.local
  ConnectTimeout 1200
